"set guifont=Hack:h15
"set guifont=DankMono-Regular:h15
"set guifont=CartographMonoCF-Regular:h14
"set guifont=PragmataProLiga-Regular:h15
"set guifont=SF\ Mono\ Powerline:h13
"set guifont=Sudo:h14
set guifont=Sudo\ Nerd\ Font:h14
"set guifont=Monoid-Retina:h14
"set guifont=Cascadia\ Code\ PL:h14
"set guifont=FiraCode-Retina:h14
" Ligatures <-> >= !== === <=
set macligatures

set number

"" Gruvbox Theme
"if (has("termguicolors"))
  "set termguicolors
"endif
"let g:gruvbox_contrast_dark="medium" " one of: soft, medium, hard
""let g:gruvbox_contrast_light="medium" " one of: soft, medium, hard
"let g:gruvbox_bold=1
"let g:gruvbox_italic=1
""let g:airline_theme='gruvbox'
""let g:gruvbox_invert_indent_guides=1
"set background=dark
"colorscheme gruvbox


"" Gruvbox Material Fork
"if (has("termguicolors"))
"  set termguicolors
"endif
"" for dark version
"set background=dark
""" for light version
"""set background=light
"" set contrast
"" this configuration option should be placed before `colorscheme gruvbox-material`
"" available values: 'hard', 'medium'(default), 'soft'
"let g:gruvbox_material_background = 'medium'
"let g:gruvbox_material_enable_bold = 1
"let g:gruvbox_material_enable_italic = 1
"let g:gruvbox_material_palette = 'mix'
"colorscheme gruvbox-material-fork
"let g:airline_theme = 'gruvbox_material'
""let g:lightline = {'colorscheme' : 'gruvbox_material'}

"" Solarized
"set background=dark
"let g:solarized_termtrans=1 " needs to be before colorscheme to remove gray background
"colorscheme solarized
"let g:airline_theme='solarized'


"" Iceberg
"set background=dark
"colorscheme iceberg
"let g:airline_theme='iceberg'

"colorscheme sourcerer
"let g:airline_theme='minimalist'

"colorscheme srcery
"let g:airline_theme='srcery'

"colorscheme tender
"let g:airline_theme='tender'

" Gruvbox Material
"" important!!
"set termguicolors
"" for dark version
"set background=dark
"" for light version
""set background=light
"" set contrast
"" this configuration option should be placed before `colorscheme gruvbox-material`
"" available values: 'hard', 'medium'(default), 'soft'
"let g:gruvbox_material_background = 'medium'
"let g:gruvbox_material_enable_bold = 1
"let g:gruvbox_material_enable_italic = 1
"let g:airline_theme = 'gruvbox_material'
"colorscheme gruvbox-material

set linespace=1
